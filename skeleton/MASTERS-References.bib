isbn = {0025-3227},
issn = {00253227},
journal = {Marine Geology},
keywords = {Catastrophic drainage,Glacial erratics,Laurentide glaciation,Mastadon and mammoth remains,Proglacial lakes,Sediment lobes,Subglacial lakes},
month = {jan},
number = {1-2},
pages = {117--145},
title = {{Drainage of late Wisconsin glacial lakes and the morphology and late quaternary stratigraphy of the New Jersey–southern New England continental shelf and slope}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0025322700001067},
volume = {172},
