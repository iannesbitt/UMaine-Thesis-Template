@article{Brune1954,
abstract = {Forty-four records of reservoir trap efficiency and the factors affecting trap efficiency are analyzed. The capacity-inflow (C/I) ratio is found to offer a much closer correlation with trap efficiency than the capacity-watershed (C/W) ratio heretofore widely used. It appears likely from the cases studied that accurate timing of venting or sluicing operations to intercept gravity underflows can treble or quadruple the amount of sediment discharged from a reservoir. Desilting basins, because of their shape and method of operation, may have trap efficiencies above 90{\%} even with very low C/I ratios. Semi-dry reservoirs with high C/I ratios, like John Martin Reservoir, may have trap efficiencies as low as 60{\%}. Truly "dry" reservoirs, such as those in the Miami Conservancy District, probably have trap efficiencies in the 10 to 40{\%} range, depending upon C/I ratio},
author = {Brune, Gunnar M.},
